{
  "name": "query-builder",
  "version": "1.0.1",
  "description": "Description for query-builder.",
  "main": "./build/main.js",
  "scripts": {
    "dev": "roamjs-scripts dev",
    "dev:dep": "roamjs-scripts dev -- --port 3100",
    "dev:roam": "cross-env ROAM_MARKETPLACE=true roamjs-scripts dev --outfile extension.js",
    "build": "roamjs-scripts build",
    "prebuild:roam": "npm install",
    "build:roam": "cross-env ROAM_MARKETPLACE=true roamjs-scripts build --outfile extension.js",
    "postbuild:roam": "mv ./build/extension.js ./"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/downloadjs": "^1.4.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "roamjs-scripts": "^0.17.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "downloadjs": "^1.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "roamjs-components": "^0.59.0"
  }
}
